{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "arrilot/laravel-widgets": "^3.12",
        "barryvdh/laravel-ide-helper": "^2.4",
        "barryvdh/laravel-snappy": "^0.4.1",
        "bnbwebexpertise/laravel-attachments": "^1.0",
        "consoletvs/charts": "^6.2",
        "cybercog/laravel-ownership": "^5.1",
        "doctrine/dbal": "^2.8",
        "edvinaskrucas/settings": "^2.0",
        "fideloper/proxy": "^4.0",
        "genealabs/laravel-model-caching": "^0.2.63",
        "h4cc/wkhtmltoimage-amd64": "^0.12.4",
        "h4cc/wkhtmltoimage-i386": "^0.12.4",
        "h4cc/wkhtmltopdf-amd64": "^0.12.4",
        "h4cc/wkhtmltopdf-i386": "^0.12.4",
        "josegonzalez/dotenv": "^3.2",
        "kris/laravel-form-builder": "^1.15",
        "laracasts/flash": "^3.0",
        "laravel/framework": "5.6.*",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.6",
        "phpmailer/phpmailer": "^6.0",
        "twilio/sdk": "^5.19"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
